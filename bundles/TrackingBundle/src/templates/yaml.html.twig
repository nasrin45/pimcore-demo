{% block content %}
    <table>
        {% for key, value in data %}
            <tr>
                <td>{{ key }}</td>
                <td>
                    {% if value is iterable %}
                        <ul>
                            {% for item in value %}
                                <li>{{ item }}</li>
                            {% endfor %}
                        </ul>
                    {% else %}
                        {{ value }}
                    {% endif %}
                </td>
            </tr>
        {% endfor %}
    </table>

{% endblock %}